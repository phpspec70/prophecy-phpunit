{
    "name": "phpspec70/prophecy-phpunit",
    "description": "PHP 7.0 support for: Integrating the Prophecy mocking library in PHPUnit test cases",
    "keywords": ["Phpunit", "Prophecy", "PHP 7.0"],
    "homepage": "http://phpspec.net",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0.0",
        "phpspec/prophecy": "^1.3",
        "phpunit/phpunit":"^6.5.14"
    },
    "autoload": {
        "psr-4": {
            "Prophecy\\PhpUnit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prophecy\\PhpUnit\\Tests\\Fixtures\\": "fixtures",
            "Prophecy\\PhpUnit\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.0"
        }
    },
    "replace": {
        "phpspec/prophecy-phpunit": "*"
    }
}